<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_list_book">
    <fragment
        android:id="@+id/navigation_list_book"
        android:name="com.example.loremperpus.ui.list_book.ListBookFragment"
        android:label="@string/list"
        tools:layout="@layout/fragment_list_book" />

    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.example.loremperpus.ui.cart.CartFragment"
        android:label="@string/cart_buku"
        tools:layout="@layout/fragment_cart" />

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.example.loremperpus.ui.history.HistoryFragment"
        android:label="@string/history"
        tools:layout="@layout/fragment_history" />
    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.loremperpus.ui.settings.SettingsFragment"
        android:label="@string/settings"
        tools:layout="@layout/fragment_notifications" />
</navigation>